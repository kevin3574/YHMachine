<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="chat_content_date_style">
        <item name="android:minHeight">30dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15sp</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">#bfbfbf</item>
    </style>

    <style name="My">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="My.ImageView">
        <item name="android:contentDescription">@string/app_name</item>
    </style>

    <style name="My.TextView">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="My.ImageView.Photo_Avatar">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="My.Button.logout" parent="My.TextView._White">
        <item name="android:background">@drawable/btn_logout_selector</item>
    </style>

    <style name="My.TextView._White">
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="Register_Edit">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:layout_marginLeft">@dimen/register_margin</item>
        <item name="android:background">@color/chat_transparent</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="Animations" />

    <!-- dialog -->
    <style name="TextAppearance">
        <item name="android:textColor">#000000</item>
        <item name="android:textColorHighlight">#FFFFFF</item>
        <item name="android:textColorHint">#FFFFFF</item>
        <item name="android:textColorLink">#FFFFFF</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="TextAppearance.DialogWindowTitle">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="DialogWindowTitle">
        <item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:textAppearance">@style/TextAppearance.DialogWindowTitle</item>
    </style>

    <style name="alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowTitleStyle">@style/DialogWindowTitle</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>


    <style name="Register_Input">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/base_edit_input</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/base_action_bar_height</item>
        <item name="android:layout_marginTop">@dimen/register_margin</item>
        <item name="android:layout_marginLeft">@dimen/register_margin</item>
        <item name="android:layout_marginRight">@dimen/register_margin</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
    </style>

    <style name="My.TextView._Black">
        <item name="android:textColor">@color/chat_base_color_text_black</item>
    </style>

    <style name="My.Button.theme" parent="My.TextView._White">
        <item name="android:background">@color/machineyellow</item>
    </style>

    <style name="My.ImageView.Photo_Middle">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:scaleType">fitXY</item>
    </style>


    <style name="My.TextView._Gray">
        <item name="android:textColor">@color/base_color_text_gray</item>
    </style>


    <style name="AnimStyle2">
        <item name="android:windowEnterAnimation">@anim/push_top_in2</item>
        <item name="android:windowExitAnimation">@anim/push_top_out2</item>
    </style>


    <style name="profile_fragment_left_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/myspace_margin_left</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="conversation_detail_base_left_style">
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="conversation_detail_base_left_layout">
        <item name="android:padding">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/group_detail_btn</item>
    </style>

    <!-- activity -->
    <style name="LCIMActivity.FullScreen">
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="mytvdialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="tv_home_item_left_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">@dimen/home_lv_paddingLeft</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/tv_size_16</item>
    </style>

    <style name="tv_home_item_right_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">2</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/tv_size_16</item>
    </style>

    <style name="white20Text">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="msg_input_et_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:background">@mipmap/input_bg</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:inputType">textPassword</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="msg_input_et_style2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:background">@mipmap/input_bg</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="msg_input_tv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/tv_size_18</item>
    </style>

    <style name="grey_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginLeft">@dimen/home_lv_paddingLeft</item>
        <item name="android:layout_marginRight">@dimen/home_lv_paddingLeft</item>
        <item name="android:background">@color/color_bottom_text_normal</item>
    </style>

    <style name="subok_but_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginTop">40dp</item>
        <item name="android:background">@color/machineyellow</item>
        <item name="android:paddingLeft">40dp</item>
        <item name="android:paddingRight">40dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="et_run_states_style">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:background">@mipmap/input_bg</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:numeric">integer</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="unit_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/tv_size_14</item>
    </style>

    <style name="radiogroup_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="radiobutton_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@drawable/radio_select</item>
        <item name="android:textColor">@color/textcolor</item>
        <item name="android:textSize">@dimen/tv_size_16</item>
    </style>

    <style name="RatingStyle" parent="@android:style/Widget.RatingBar">

        <!-- 定义星星图片 -->
        <item name="android:progressDrawable">@drawable/rating_bar</item>
        <!-- 根据自定义星星图片的大小,设置相应的值,否则可能显示不全 -->
        <item name="android:minHeight">30dp</item>
    </style>

    <style name="good_say_detail_rl_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingTop">15dp</item>
    </style>

    <style name="good_say_detail_tv_left_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">55dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/tv_size_20</item>
    </style>

    <style name="good_say_detail_tv_right_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">50dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/tv_size_20</item>
    </style>
    <style name="chat_play_btn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="chat_avatar">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:clickable">true</item>
        <item name="android:visibility">visible</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:src">@drawable/chat_default_user_avatar</item>
    </style>

    <style name="chat_record_button_toast_dialog_style">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@drawable/chat_record_button_round_background</item>
    </style>

</resources>
